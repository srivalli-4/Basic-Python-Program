marks=int(input("enter your marks"))
if (marks>=90):
  print("A")
  print("Excellent")
elif(marks>=80 and marks<90):
  print("B")
  print("Good")
elif(marks>=50 and marks<80):
  print("C")
  print("Average")
elif(marks>=40 and marks<50):
  print("D")
  print("Require improvement")
else:
  print("F")
